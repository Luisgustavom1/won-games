    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "jest-styled-components": "^7.1.1",
    "jsdom": "^22.0.0",
    "lint-staged": "^14.0.0",
    "plop": "^3.1.2",
    "prettier": "2.8.8",
